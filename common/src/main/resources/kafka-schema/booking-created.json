{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "BookingCreated",
  "type": "object",
  "properties": {
    "bookingId": {
      "type": "string"
    },
    "traceId": {
      "type": "string"
    },
    "user": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer"
        },
        "username": {
          "type": "string"
        },
        "fullName": {
          "type": "string"
        }
      }
    },
    "movie": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer"
        },
        "title": {
          "type": "string"
        }
      }
    },
    "seatCodes": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "status": {
      "type": "string"
    },
    "createdAt": {
      "type": "string",
      "format": "date-time"
    }
  },
  "required": [
    "bookingId",
    "user",
    "movie",
    "seatCodes",
    "status",
    "createdAt"
  ]
}